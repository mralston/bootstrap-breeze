{
    "name": "mralston/bootstrap-breeze",
    "description": "Minimal Laravel authentication scaffolding with Blade or Vue/Inertia & Bootstrap. This unofficial package is based on laravel/breeze by Taylor Otwell.",
    "keywords": ["laravel", "auth", "bootstrap"],
    "license": "MIT",
    "support": {
        "issues": "https://github.com/mralston/bootstrap-breeze/issues",
        "source": "https://github.com/mralston/bootstrap-breeze"
    },
    "authors": [
        {
            "name": "Taylor Otwell",
            "email": "taylor@laravel.com"
        },
        {
            "name": "Matt Ralston",
            "email": "matt@mralston.co.uk"
        }
    ],
    "require": {
        "php": "^7.3|^8.0",
        "illuminate/filesystem": "^8.42",
        "illuminate/support": "^8.42",
        "illuminate/validation": "^8.42",
        "laravel/breeze": "^1.3"
    },
    "autoload": {
        "psr-4": {
            "Mralston\\BootstrapBreeze\\": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        },
        "laravel": {
            "providers": [
                "Mralston\\BootstrapBreeze\\BootstrapBreezeServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
